<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="android-suite" parallel="tests" thread-count="3">

    <parameter name="jenkinsJobName" value="android-tests"/>
    <parameter name="jenkinsJobType" value="android"/>
    <parameter name="jenkinsEnvironments" value="DEMO"/>

    <test name="chrome-test" parallel="methods" thread-count="2" >
        <parameter name="browserName" value="chrome"/>
        <classes>
            <class name="com.solvd.posteducationtaskscarina.AmazonAndroidTest">
            </class>
        </classes>
    </test>
    <test name="firefox-test" parallel="methods" thread-count="2">
        <parameter name="browserName" value="firefox"/>
        <classes>
            <class name="com.solvd.posteducationtaskscarina.AmazonAndroidTest">
            </class>
        </classes>
    </test>
</suite>